geomesa {
  sfts {
    telemetry_status = {
      fields = [
        { name = time,            type = Timestamp, default = true }
        { name = mid,             type = Integer }
        { name = assetId,         type = String,    index = true }
        { name = deviceId,        type = String }
        { name = familyCode,      type = String }
        { name = confidential,    type = Boolean }
        { name = translatedValue, type = Double }
        { name = offset,          type = Double }
        { name = samplingPeriod,  type = Integer }
        { name = valueType,       type = String }
        { name = reportType,      type = String }
        { name = resolution,      type = Double }
        { name = parameterType,   type = String }
        { name = techType,        type = String }
        { name = issigned,        type = String }
        { name = to,              type = String }
        { name = from,            type = String }
        { name = decodedValue,    type = String }
        { name = last,            type = Double }
        { name = min,             type = Double }
        { name = max,             type = Double }
        { name = sum,             type = Double }
        { name = cnt,             type = Integer }
        { name = line,            type = Integer }
      ]
    }
  }
}



